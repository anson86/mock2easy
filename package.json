{
  "name": "mock2easy",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "watch": "watchify -t brfs public/javascripts/app.js -o public/build/bundle.js -v",
    "build": "browserify -t brfs  public/javascripts/app.js -o public/build/bundle.js",
    "util": "browserify -t brfs  public/javascripts/util.js -o public/build/util.js",
    "start": "npm run watch & nodemon ./bin/www"
  },
  "dependencies": {
    "body-parser": "~1.0.0",
    "brfs": "^1.4.1",
    "browserify-middleware": "^7.0.0",
    "cookie-parser": "~1.0.1",
    "debug": "~0.7.4",
    "express": "~4.2.0",
    "insert-css": "^0.2.0",
    "jade": "~1.3.0",
    "morgan": "~1.0.0",
    "nodemon": "^1.4.1",
    "static-favicon": "~1.0.0",
    "watchify": "^3.4.0"
  },
  "devDependencies": {
    "angular": "^1.4.5",
    "angular-sanitize": "^1.4.5",
    "angular-translate": "^2.7.2",
    "angular-ui-bootstrap": "^0.13.4",
    "angular-ui-router": "^0.2.15",
    "bootstrap": "^3.3.5",
    "json2": "^0.4.0",
    "mockjs": "^0.1.10"
  }
}
