

var options =  {
  port:5000,
  lazyLoadTime:3000,
  database:'mock2easy',
  doc:'doc',
  keepAlive:true,
  isSpider:false,
  ignoreField:[],
  interfaceSuffix:'.json',
  preferredLanguage:'en'
};


